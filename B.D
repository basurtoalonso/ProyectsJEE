        INSERT INTO CALLCEN.LIBERA_BIOM
        (
            FOLIO,
            ESTATUS,
            PAIS_SOLICITANTE,
            USUARIO_MODIFICO,
            FECHA_MODIFICACION,
            AGENTE,
            FECHA_REGISTRO
        )
        VALUES
        (
            VFOLIO,
            VPENDIENTE,
            VPAIS,
            VUSER,
            SYSDATE,
            VNOASIGNADO,
            SYSDATE
        );      
        INSERT INTO CALLCEN.BITACORA_BIOM
        (
            FOLIO,
            ESTATUS,
            AGENTE,
            CUENTA_CLIENTE,
            PAIS_SOLICITANTE,
            USUARIO_MODIFICO,
            FECHA_MODIFICACION,
            FECHA_REGISTRO,
            SUCURSAL,
            CANAL_GESTORA,
            SUCURSAL_GESTORA,
            CTE_UNICO,
            ID_PRODUCTO_FULL,
            NOMBRE_SUCURSAL,
            REC_FACIAL,
            TIP_IDENTIFICACION,
