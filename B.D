	   	FUNCTION F_call_Existe_Ip_rs ( V_IP IN VARCHAR DEFAULT NULL ) RETURN Types.cursorType IS

			cur Types.cursorType;
			BEGIN
				IF V_IP IS NULL THEN
     			   OPEN cur FOR
					  	SELECT NVL(S.AGENTE,'NULO') AS EXISTE
					  	FROM CALLCEN.CAT_IPS S
						WHERE S.IP IS NULL;
				   RETURN cur;
				   CLOSE cur;
			    ELSE
