V_ESTADO_1                  IN    varchar2,
V_ESTADO_2                  IN    varchar2,
V_ESTADO_3                  IN    varchar2,
PSCod_Error                 OUT    number,
PSMsg_Error                 OUT    varchar2,
CURSOR_CLIENTE              OUT    sys_refcursor
)
IS
BEGIN
    OPEN CURSOR_CLIENTE FOR   
