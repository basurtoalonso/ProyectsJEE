                ,STATUS
                ,SISTEMA
                ,PAIS
                ,USUARIO_MODIFICO
            FROM EBANKING.CU_FRECUENTES
           WHERE ID > CSLCERO
             AND ID_ALNOVA = paFcAlnovaUser
             AND STATUS IN( CSLACTIVO,CSLINACTIVO );
    RETURN CURSOR_OUTPUT;
EXCEPTION
