  RETURN INTEGER IS
    v_fecha DATE;
    v_sesion VARCHAR(100);
    v_estado INTEGER;
BEGIN

    SELECT session_start, session_activa
        INTO v_fecha, v_sesion
        FROM ebanking.ebanking_user_session
        WHERE id=p_userId;

    IF (v_fecha + NUMTODSINTERVAL(1, 'HOUR')) > SYSDATE
