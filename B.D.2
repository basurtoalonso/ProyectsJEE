            values (pClient, pDestino, pName, 1,      sysdate,            pIpOrigen, pEmail, pCelular, pTelCasa, pTelOficina, pExtension);
	  RETURN 1;
  END;

  --Si el registro existe y el Status es ACTIVO, entonces no se realiza nada...
  IF vStatus = 1 THEN
