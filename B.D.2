			 	A.OFFICE_PHONE		  = NVL(PEREG.OFFICE_PHONE,A.OFFICE_PHONE), 
		 	 	A.STATUS			  = NVL(PEREG.STATUS,A.STATUS),
			 	A.ACTIVACION		  = NVL(PEREG.ACTIVACION,A.ACTIVACION), 
			 	A.CODE				  = NVL(PEREG.CODE,A.CODE),
