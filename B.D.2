  pCliente INTEGER,
  pOldTelefono NUMBER,
  pOldCarrier NUMBER,
  pNewTelefono NUMBER,
  pNewCarrier NUMBER,
  pClave INTEGER
  )

  RETURN INTEGER IS

  vTelefono NUMBER;
  vMensajeID NUMBER;
  vMensaje NUMBER;
  verdadero INTEGER:= 1;
  falso INTEGER:= 0;

  BEGIN
	BEGIN
	  -- Si no existe cliente en la base de datos regresar ERROR
