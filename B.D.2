			 	A.CONFIRMACION	  	  = NVL(PEREG.CONFIRMACION,A.CONFIRMACION)            
	      WHERE A.ALIAS = PEREG.ALIAS;
	  END IF;
      COMMIT;
	  PSCOD:=0;
	  PSMSG:='OPERACION EXITOSA';
