        SET EBANKING.EBANKING_ACTIVATION.EMAIL = MAIL_IN
        WHERE EBANKING.EBANKING_ACTIVATION.CLIENTE = IDALNOVA_ACT;
            
            
        IF SQL%FOUND THEN
            SALIDA2 := 'CORRECTO1 '|| SALIDA3; 
        ELSE
            SALIDA2:='ERROR1 '|| SALIDA3;
        END IF;
        
        
            
        EXCEPTION  
