CREATE OR REPLACE PROCEDURE          SP_UPD_USR_ACT(
             IDALNOVA            IN    NUMBER
            ,IDALNOVA_ACT       IN    VARCHAR
            ,MAIL_IN               IN    VARCHAR2
            ,SALIDA1             OUT   VARCHAR2
            ,SALIDA2             OUT   VARCHAR2
)
