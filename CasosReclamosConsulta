     function showReclamoConsulta(){
         var valor = form.option.value;
         //var valor = document.form.caso.value;
         if(valor == 1){ 
         document.getElementById("muestraFormReclamo").style.display="block";
         document.getElementById("muestraFormConsulta").style.display="none";
         //cargarIdBien();//CARGA POR DEFAUL EL WS PRODUCTO SERVICIO ID BIEN CONTRATADO
        } else if(valor == 2){
          var consulta = document.getElementById("muestraFormConsulta").style.display="block";	
          document.getElementById("muestraFormReclamo").style.display="none";
	     }
     }
     
