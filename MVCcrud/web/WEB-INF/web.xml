<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>controllerUpdate</servlet-name>
        <servlet-class>Servlets.controllerUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controllerInsert</servlet-name>
        <servlet-class>Servlets.controllerInsert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controllerDelete</servlet-name>
        <servlet-class>Servlets.controllerDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>controllerUpdate</servlet-name>
        <url-pattern>/controllerUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controllerInsert</servlet-name>
        <url-pattern>/controllerInsert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controllerDelete</servlet-name>
        <url-pattern>/controllerDelete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
