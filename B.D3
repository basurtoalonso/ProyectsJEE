          p_fecha_fin IN varchar2, 
          p_tipo_membresia IN varchar2, 
          p_promedio_tipo_cambiario IN varchar2,
          p_cursor        OUT      sys_refcursor)
is
/* ------------------------------------------------------------------------------------- */
/* NOMBRE PROCEDIMIENTO: MOD_REPORTE_FONDEOS                                             */
