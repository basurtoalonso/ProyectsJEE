     EXCEPTION
     WHEN v_error THEN
     BEGIN
        --RESULTADO := 0;
        ROLLBACK;
        RAISE_APPLICATION_ERROR (-20100, 'Error ocurrido al Insertar en ICN_CONTRATO   (' || SQLCODE || '  ' || SQLERRM || ')');
    
