    P_ACTUALCARDNUMBER   IN    CARTCARD.T_P_ACTIVADAS.ACTUALCARDNUMBER%TYPE,
    P_CARDNUMBER         IN    CARTCARD.T_P_ACTIVADAS.CARDNUMBER%TYPE,
