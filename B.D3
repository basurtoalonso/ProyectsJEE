                   CARTCARD.cc_slcaclaracion.numeroregistro
                  )
           VALUES (v_numeroaclaracion, p_usuarioid, SYSDATE, p_asunto,
                   p_comentario, p_estatusaclaracion, p_numeroregistro
                  );
