        resultadotex :=  'No se Realizo ninguna accion'; 

    END IF;

   COMMIT;
EXCEPTION
   WHEN OTHERS THEN
     resultado := 0;
     resultadotex := SQLCODE || '  ' || SQLERRM;
      ROLLBACK;
      raise_application_error (-20120,'Error ocurrido en PKG_ICN_CATALOGOS.CAT_TIPO_BLOQUEO(' || SQLCODE|| ''|| SQLERRM || ')');
