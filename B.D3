                               CARTCARD.OPERACIONVENTANILLA.ip,
                               CARTCARD.OPERACIONVENTANILLA.fechasys,
                               CARTCARD.OPERACIONVENTANILLA.tipocambiousd
                              )
                       VALUES (p_operacionid,
                               v_consecutivo,
                               p_numerosucursal,
                               p_numerooperador,
                               TO_DATE (p_fechaoperacion,
                                        'ddMMYYYY-HH24:MI:ss'
                                       ),
                               p_codigoresultado,
                               p_mensajeresultado,
                               p_numeroreferencia,
                               p_montofondeado,
