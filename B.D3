   -- FONDEO POR VENTANILLA
   SELECT OV.FECHASYS AS FECHA, T.NUMEROTARJETA, OV.SUCURSALID, TO_CHAR(OV.FECHASYS, 'yy') as ANIO,
         TO_CHAR(OV.FECHASYS, 'ww') AS SEMANA, 
