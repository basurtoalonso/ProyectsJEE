EXCEPTION
   WHEN vlerror then
     PSCod_Error:=0;
     PSMsg_Error:='El Numero de Tarjeta:'||PENumTarjeta||' ya esta dado de alta';
  ROLLBACK;      
   WHEN others THEN
     PSCod_Error:=0;
