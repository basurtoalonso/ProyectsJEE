     B.PLASTNAME,
     NVL(NULL,' ') "PMAINDENNAME",
     NVL(NULL,' ') "PMIDDLENAME",
     B.PFIRSTNAME,
     B.PCOUNTRY,
     B.PSTATE,
     B.PCITY,
     B.PADDRESS,
     B.PZIPCODE,
