/* Fecha Modificacion:                                                                                                             */
/* Ejemplo Ejecucion: exec AddTarjetaExpirada(<<usuarioid>>,<<:cartcardid>>)        */
/* --------------------------------------------------------------------------------------------------------------- */
   v_referencia            VARCHAR2(11);
