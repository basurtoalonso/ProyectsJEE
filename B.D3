   p_numeroaclaracion    IN       INTEGER,
   p_asunto              IN       VARCHAR2,
   p_comentario          IN       CLOB,
   p_estatusaclaracion   IN       INTEGER,
   p_numeroregistro      OUT      INTEGER
)
IS
   v_numeroaclaracion   INTEGER;
/* ---------------------------------------------------------------------------------------------------------------------------------- */
/* NOMBRE PROCEDIMIENTO: AGREGASOLICITUDACLARACION                                                                                    */
