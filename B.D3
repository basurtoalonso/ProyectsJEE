CREATE OR REPLACE PROCEDURE icnaddtarjetaexpirada2 (
     p_referencia   IN      cartcard.icn_tarjeta_complemento.referencia%type,
     p_usuarioid    IN      cartcard.icn_contrato.id_usuario%type,
